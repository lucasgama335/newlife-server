hook OnPlayerConnect(playerid)
{
	//=====================================[ REMOVE WORLD OBJECTS ]====================================||

    //=====================================[ MAP ICONS ]====================================||
	SetPlayerMapIcon(playerid, m_MAP_ICON_PRISON, PRISON_ENTRANCE, 30 , 0xE6E6E6E6);
	return 1;
}

hook OnPlayerPressF(playerid)
{
	// Entrance
	if (IsPlayerInRangeOfPoint(playerid, 3.0, PRISON_ENTRANCE))
	{
        GameTextForPlayer(playerid, "~w~penitenciaria", 3000, 1);
		TeleportPlayer(playerid, PRISON_EXIT, PRISON_LOCALE, 0, PRISON_LOCALE, true, 3000);
		return 1;
   	}
	// Exit
	if (IsPlayerInRangeOfPoint(playerid, 3.0, PRISON_EXIT))
	{
	    StopAudioStreamForPlayer(playerid);
		TeleportPlayer(playerid, PRISON_ENTRANCE, 0, 0, NO_INTERIOR);
		return 1;
	}
	return 1;
}
