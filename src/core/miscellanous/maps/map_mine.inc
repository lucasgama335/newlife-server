hook OnPlayerPressF(playerid)
{
	// Entrance
	if (IsPlayerInRangeOfPoint(playerid, 3.0, MINE_ENTRANCE))
	{
        GameTextForPlayer(playerid, "~w~mina", 3000, 1);
		TeleportPlayer(playerid, MINE_EXIT, MINE_LOCALE, 0, MINE_LOCALE, true, 3000);
		return 1;
   	}
	// Exit
	if (IsPlayerInRangeOfPoint(playerid, 3.0, MINE_EXIT))
	{
	    StopAudioStreamForPlayer(playerid);
		TeleportPlayer(playerid, MINE_ENTRANCE, 0, 0, NO_INTERIOR);
		return 1;
	}
	return 1;
}
