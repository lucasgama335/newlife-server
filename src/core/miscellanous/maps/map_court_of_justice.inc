hook OnPlayerPressF(playerid)
{
	// Entrance
	if (IsPlayerInRangeOfPoint(playerid, 3.0, COURT_OF_JUSTICE_ENTRANCE))
	{
        GameTextForPlayer(playerid, "~w~tribunal de justica", 3000, 1);
		TeleportPlayer(playerid, COURT_OF_JUSTICE_EXIT, COURT_OF_JUSTICE, 0, COURT_OF_JUSTICE, true, 3000);
		return 1;
   	}
	// Exit
	if (IsPlayerInRangeOfPoint(playerid, 3.0, COURT_OF_JUSTICE_EXIT))
	{
	    StopAudioStreamForPlayer(playerid);
		TeleportPlayer(playerid, COURT_OF_JUSTICE_ENTRANCE, 0, 0, NO_INTERIOR);
		return 1;
	}
	return 1;
}
