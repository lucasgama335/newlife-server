{
    if (IsValidVehicle(vehicleid))
    {
        Vehicle_SetHealth(vehicleid, 1000.0);
        SetVehicleHealth(vehicleid, 1000.0);
        RepairVehicle(vehicleid);
    }
    return 1;
}

