{
	"user": "lucasgama335",
	"repo": "NewLife_Server",
	"entry": "src/main.pwn",
	"output": "gamemodes/main.amx",
	"dependencies": [
		"pawn-lang/samp-stdlib",
		"pawn-lang/sa-mp-fixes",
		"pawn-lang/YSI-Includes",
		"Zeex/samp-plugin-crashdetect:v4.20",
		"pBlueG/SA-MP-MySQL:R41-4",
